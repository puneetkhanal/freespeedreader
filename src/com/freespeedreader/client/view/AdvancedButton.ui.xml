<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.spinner {
			margin: 3px 0px 0px 5px;
		}
	</ui:style>
<ui:with type="com.freespeedreader.client.resources.FreespeedreaderResources"
		field="resources"/>
	<g:HorizontalPanel>
		<g:Image width="26px" ui:field="image" resource="{resources.iconTimer}" styleName="{style.spinner}" />
		<g:PushButton ui:field="button"/>
	</g:HorizontalPanel>
</ui:UiBinder> 